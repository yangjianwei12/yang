<!--Chain for LE Unicast Mono DECODER with WB CVC -->

<chain name="CHAIN_LC3_ISO_TO_DAC_VOICE_WB_MONO" id="0" generate_operator_roles_enum="False" generate_endpoint_roles_enum="False">

    <include_header name="app_chain_config.h"/>
    <include_header name="kymera_chain_roles.h"/>

    <operator name="OPR_LC3_DECODE_SCO_ISO" id="CAP_ID_LC3_SCO_ISO_DEC">
        <sink name="IN" terminal="0"/>
        <source name="OUT" terminal="0"/>
    </operator>

    <operator name="OPR_CVC_RECEIVE" id="CAP_ID_CVC_RECEIVE_WB" priority="low">
        <sink name="IN" terminal="0"/>
        <source name="OUT" terminal="0"/>
    </operator>

    <operator name="OPR_LEA_USB_SPLT_ISO_RX" id="CAP_ID_SPLITTER">
        <sink name="IN" terminal="0"/>
        <source name="OUT_L" terminal="0"/>
        <source name="OUT_R" terminal="1"/>
    </operator>

    <input sink="OPR_LC3_DECODE_SCO_ISO.IN" role="EPR_ISO_FROM_AIR_LEFT"/>
    <connection source="OPR_LC3_DECODE_SCO_ISO.OUT" sink="OPR_CVC_RECEIVE.IN"/>
    <connection source="OPR_CVC_RECEIVE.OUT" sink="OPR_LEA_USB_SPLT_ISO_RX.IN"/>
    <output source="OPR_LEA_USB_SPLT_ISO_RX.OUT_L" role="EPR_SOURCE_DECODED_PCM"/>
    <output source="OPR_LEA_USB_SPLT_ISO_RX.OUT_R" role="EPR_SOURCE_DECODED_PCM_RIGHT"/>
</chain>



