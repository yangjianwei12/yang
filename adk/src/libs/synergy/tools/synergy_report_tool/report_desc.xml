			<COLUMN name='readAttrDirect' title='Is Reading Direct Attributes'/>
		</TABLE>
		<TABLE title='LE REPORT' data_type='linked_list_ptr' name='leVar.connCache' next_ptr='next' lvl1_element_type='leConnVar' add_to_detailed_report='true'>
			<COLUMN name='addr.addr' title='Device Address' data_type='CsrBtDeviceAddr'/>
			<COLUMN name='master' title='Is Master Role'/>
			<COLUMN name='idAddr.addr' title='ID Address' data_type='CsrBtDeviceAddr'/>
			<COLUMN name='connParams.supervision_timeout' title='Supervision Timeout'/>
		</TABLE>
		<TABLE title='PENDING LIST' data_type='CsrCmnListSimple_t_ptr' name='pendingMsgs' next_ptr='elem.next' lvl1_element_type='cmPendingMsg_t' add_to_detailed_report='true'>
			<COLUMN name='type' title='Pending Message Type'/>
		</TABLE>
	</REPORT>
		<REPORT group='av_report' global_name='csrBtAvInstData' title='Synergy A2DP Profile Report'>
		<TABLE title='AV Main Instance Report' data_type='regular'>
			<COLUMN name='appHandle' title='Application Handle' data_type='TaskHandler'/>
			<COLUMN name='state' title='AV Main Instance State'/>
