			<COLUMN name='dmVar.state' title='State'/>
			<COLUMN name='dmVar.lockMsg' title='Message Locking DM'/>
		</TABLE>
		<TABLE title='ACL REPORT' data_type='array' name='roleVar.aclVar' length='3' add_to_detailed_report='true'>
