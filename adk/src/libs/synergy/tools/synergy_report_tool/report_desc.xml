			<COLUMN name='hfgConfig' title='Profile setup bitmask'/>	
			<COLUMN name='maxConnections' title='Max concurrent connections'/>
		</TABLE>
		<TABLE title='HFG Instance Report' data_type='array' name='linkData' length='2' add_to_detailed_report='true'>
			<COLUMN name='index' title='Instance Index'/>
			<COLUMN name='state' title='Connection State' data_type='HfgConnectionState_t'/>
			<COLUMN name='oldState' title='Old Connection State' data_type='HfgConnectionState_t'/>
			<COLUMN name='atState' title='AT sub-state' data_type='HfgAtState_t'/>
			<COLUMN name='hfgConnId' title='Connection ID'/>
