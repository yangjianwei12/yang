		<TABLE title='RFC REPORT' data_type='Instanced_CsrCmnListSimple_t' name='rfcVar.connList' lvl1_element_type='cmRfcConnElement' lvl2_element_type='cmRfcConnInstType' lvl2_element_ptr='cmRfcConnInst' add_to_detailed_report='true'>
