			<COLUMN name='searchOngoing' title='Ongoing SDP Search'/>
			<COLUMN name='isConnectable' title='Outstanding Connect Accept'/>
		</TABLE>
		<TABLE title='AV Connection Information' data_type='array' name='con' length='2' add_to_detailed_report='true'>
			<COLUMN name='remoteDevAddr' title='Remote Device Address' data_type='CsrBtDeviceAddr'/>
			<COLUMN name='signalCid' title='Local L2CAP Signaling CID'/>
			<COLUMN name='remoteCid' title='Remote L2CAP Signaling CID'/>
			<COLUMN name='conState' title='Connection State'/>
			<COLUMN name='aclHandle' title='ACL Connection Handle'/>
			<COLUMN name='incoming' title='Incoming'/>
			<COLUMN name='pendingSigProc' title='Pending Signaling Procedure'/>
		</TABLE>
	    <TABLE title='AV Stream Information' data_type='array' name='stream' length='2' add_to_detailed_report='true'>
			<COLUMN name='mediaCid' title='Local L2CAP Media CID'/>
			<COLUMN name='remoteCid' title='Remote L2CAP Media CID'/>
			<COLUMN name='remoteSeid' title='Remote Device SEID'/>
			<COLUMN name='localSeid' title='Local Device SEID'/>
			<COLUMN name='conId' title='Connection Information Index'/>
			<COLUMN name='streamState' title='Stream State'/>
		</TABLE>
	</REPORT>
	<REPORT group='hfg_report' global_name='csrBtHfgInstance' title='Synergy HFG Profile Report'>
