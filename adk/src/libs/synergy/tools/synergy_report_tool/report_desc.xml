			<COLUMN name='btConnId' title='Bluetooth Connection ID'/>
			<COLUMN name='serverChannel' title='Local Server Channel'/>
			<COLUMN name='remoteServerChan' title='Remote Server Channel'/>
			<COLUMN name='dlci' title='DLCI'/>
			<COLUMN name='appHandle' title='Application Handle' data_type='TaskHandler'/>
			<COLUMN name='context' title='Context'/>
			<COLUMN name='state' title='RFC State' data_type='CsrBtCmStateRfc'/>
		</TABLE>
		<TABLE title='SDC REPORT' data_type='CsrCmnListSimple_t' name='sdcVar.sdcSearchList' next_ptr='elem.next' lvl1_element_type='sdcSearchElement' add_to_detailed_report='true'>
			<COLUMN name='deviceAddr' title='Device Address' data_type='CsrBtDeviceAddr'/>
			<COLUMN name='appHandle' title='Application Handle' data_type='TaskHandler'/>
			<COLUMN name='uuidCount' title='UUID Count'/>
			<COLUMN name='uuidType' title='UUID Type'/>
