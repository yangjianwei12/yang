			<COLUMN name='address' title='Peer address' data_type='CsrBtDeviceAddr'/>
		</TABLE>
		<TABLE title='HFG Instance Report...contd' data_type='array' name='linkData' length='2' add_to_detailed_report='true'>	
			<COLUMN name='index' title='Instance Index'/>			
			<COLUMN name='remSupFeatures' title='Remote Supported Features Mask'/>
			<COLUMN name='scoHandle' title='SCO connection handle'/>
			<COLUMN name='remoteVersion' title='Remote Version'/>
			<COLUMN name='remSupFeatures' title='Remote Supported Features Mask'/>
			<COLUMN name='remSupportedCodecs' title='Remote Supported Codecs Mask'/>
			<COLUMN name='selectedQceCodecId' title='Selected QCE Codec'/>
		</TABLE>
		<TABLE title='HFG Instance Report...pending info' data_type='array' name='linkData' length='2' add_to_detailed_report='true'>	
			<COLUMN name='index' title='Instance Index'/>
			<COLUMN name='searchAndCon' title='SDS search and connection'/>
			<COLUMN name='pendingCodecNegotiation' title='Codec Negotiation'/>
			<COLUMN name='pendingDisconnect' title='Disconnect'/>
			<COLUMN name='pendingPeerDisconnect' title='Peer Disconnection'/>
			<COLUMN name='pendingSco' title='SCO connection'/>
			<COLUMN name='pendingScoConfirm' title='SCO requested by App'/>
			<COLUMN name='pendingScoDisconnect' title='SCO disconnect by App'/>
		</TABLE>
	</REPORT>
	<REPORT group='gatt_report' global_name='gattMainInstance' title='Synergy GATT Report'>
